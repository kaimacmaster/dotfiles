# User configuration
export EDITOR='nvim'

# Aliases
alias ll='ls -alF'
alias yd='nvm use && yarn dev'
alias yid='nvm use && yarn && yarn dev'
alias nv='nvim'
alias gs='git status'
alias gc='git commit -a -m'
alias ga='git add'
alias gp='git push'
alias gd='git diff'
alias gl='git log --left-right --decorate --graph'
alias zshrc='nvim ~/.zshrc'

